10 OPEN #1:"RS232/1.BA=9600.DA=8.PA=E.EC.LF",DISPLAY ,VARIABLE 254
20 LINPUT #1:FILENAME$
30 PATH$="DSK1."&FILENAME$
40 PRINT PATH$
50 OPEN #2:PATH$,SEQUENTIAL,DISPLAY ,OUTPUT,VARIABLE 254
60 PRINT #1:"OK"
70 PRINT "SENT OK"
80 LINPUT #1:LINE$
90 IF LINE$="### END OF FILE" THEN 140
100 PRINT LINE$
110 PRINT #2:LINE$
120 PRINT #1:"OK"
130 GOTO 80
140 CLOSE #2
150 CLOSE #1
160 GOTO 10
