10 OPEN #1:"RS232/1.BA=9600.DA=8.PA=E.EC",DISPLAY ,VARIABLE 254
20 LINPUT #1:FILENAME$
30 PATH$="DSK1."&FILENAME$
40 PRINT PATH$
50 OPEN #2:PATH$,SEQUENTIAL,DISPLAY ,OUTPUT,VARIABLE 254
60 LINPUT #1:LINE$
70 IF ASC(SEG$(LINE$,1,1))=10 THEN LINE$=SEG$(LINE$,2,LEN(LINE$)-1)
80 IF LINE$="### END OF FILE" THEN 130
90 PRINT LINE$
100 PRINT #2:LINE$
110 PRINT #1:"OK"
120 GOTO 60
130 CLOSE #2
140 CLOSE #1
150 END
